#include "myGPIO.h"



uint8_t GPIO_Led_BootStart_Notify(void)
{
	for (uint8_t i = 0; i < 10; i++)
	{
	  HAL_GPIO_TogglePin(LED_START_GPIO_Port, LED_START_Pin);
	  HAL_Delay(200);
	}
	HAL_GPIO_WritePin(LED_START_GPIO_Port, LED_START_Pin, GPIO_PIN_SET);
 	return 1;
}

