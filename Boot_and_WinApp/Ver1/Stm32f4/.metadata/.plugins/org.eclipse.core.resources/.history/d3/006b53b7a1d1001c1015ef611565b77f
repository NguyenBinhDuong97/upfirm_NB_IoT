#ifndef APPLICATION_H_
#define APPLICATION_H_

#include "main.h"
#include "stdint.h"

typedef enum{
	_INITIAL_,
	_PUSH_BUTTON_,
	_START_,
	_BOOTING,
	_END_,
	_CHECK_RECEIVE_DONE_,
	_CHECK_RECEIVE_,
	_CALL_APPLICATION_,
} EVENT;

typedef uint8_t event;
typedef uint8_t pre_event;

void Handle_Event (event Event);
#endif /* APPLICATION_H_ */
